{
  "headers": [
    {
      "source": "/riot.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/update-ranking",
      "schedule": "0,15,30,45 * * * *"
    }
  ]
}

